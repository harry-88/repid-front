{
  "name": "rapid-front",
  "version": "1.0.3",
  "description": "A CLI tool that automates the bridge between Swagger (OpenAPI) documentation and frontend state management/API integration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "rapid-front": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc && node scripts/copy-templates.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "swagger",
    "openapi",
    "cli",
    "code-generator",
    "zustand",
    "redux",
    "axios",
    "frontend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/harry-88/repid-front"
  },
  "bugs": {
    "url": "https://github.com/harry-88/repid-front/issues"
  },
  "homepage": "https://github.com/harry-88/repid-front#readme",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "axios": "^1.6.5",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "prettier": "^3.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
